<%# Formulario edit y new %>
<br><br><br><br><br>

<%= simple_form_for(@car) do |f| %>
  <%= f.input :modelo %>
  <%= f.input :anio %>
  <%= f.input :precio %>
  <%= f.input :ubicacion %>
  <%= f.input :detalles %>
  <%= f.input :capacidad %>
  <%= f.input :photo, as: :file %>

  <%= f.submit %>
<% end %>
