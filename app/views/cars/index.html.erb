<h1> My Cars </h1>

<% @cars.each do |car| %>
<ul>
<li><%= link_to car.modelo, car_path(car) %>  <%= link_to "Edit", edit_car_path(car) %>   <%= link_to "Destroy", car, method: :destroy, data: { confirm: "Are you sure you want to delete this car?" } %></li>
</ul>
<% end %>
