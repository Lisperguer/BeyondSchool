<h1> My Cars </h1>

<% @cars.each do |car| %>
<ul>
<li><%= link_to car.modelo, car_path(car), class: 'btn btn-outline-secondary' %>  <%= link_to "Edit", edit_car_path(car), class: 'btn btn-outline-secondary' %>   <%= link_to "Destroy", car_path(car), method: :delete, data: { confirm: "Are you sure you want to delete this car?" }, class: 'btn btn-outline-secondary'%></li>
</ul>
<% end %>
<%= link_to "add new car", new_car_path, class: 'btn btn-outline-secondary' %>

